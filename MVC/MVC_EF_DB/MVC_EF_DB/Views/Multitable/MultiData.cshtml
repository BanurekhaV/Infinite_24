@model IEnumerable<MVC_EF_DB.Models.Order>
@{
    ViewBag.Title = "MultiData";
}

<h2>MultiData</h2>

@foreach(var item in Model)
            {
                <ul>
                    <li>Order ID : @item.OrderID</li>
                    <li>Customer Name : @item.Customer.CompanyName</li>
                    <li>Employee Last Name : @item.Employee.LastName</li>
                    <li>Shipper Name : @item.Shipper.CompanyName</li>
                </ul>
            }